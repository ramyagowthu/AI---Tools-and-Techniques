   ],
   "source": [
    "#y=mx+c . To get the values of m and c\n",
    "num=0\n",
    "den=0\n",
    "for i in range(n):\n",
    "    num=num+((x[i]-mean_x)*(y[i]-mean_y))\n",
    "    den=den+((x[i]-mean_x)**2)\n",
    "m=num/den\n",
    "#c=y-m*x\n",
    "c=mean_y-(m*mean_x)\n",
    "print(m,c)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Calculate y_prediction\n",
    "y_pred=np.array([]) #To create empty array\n",
    "for i in range(n):\n",
    "    p=(m*x[i])+c #y=mx+c\n",
    "    y_pred=np.append(y_pred,np.array([p]),axis=0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Plot the regression line\n",
    "plt.plot(x,y_pred,color=\"m\",label=\"regression line\")\n",
    "#Plot the actual data\n",
    "plt.scatter(x,y,40,color=\"g\",label=\"actual_data\",marker=\"o\")\n",
    "plt.xlabel(\"population\")\n",
    "plt.ylabel(\"profit\")\n",
    "plt.legend()\n",
    "plt.show()"
   ]
